# Environment variables for Llamasearch ExperimentalAgents

# OpenAI API Key (Required for OpenAI models and embeddings)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"

# Optional: Keys for other LLM providers if using multi-LLM strategy
# ANTHROPIC_API_KEY="YOUR_ANTHROPIC_API_KEY_HERE"
# GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE"

# Optional: Database path (defaults usually work)
# SQLITE_DB_PATH="local.db"

# Optional: Haystack/LlamaIndex configuration if needed
# HAYSTACK_CONFIG_PATH="path/to/haystack_config.yml" 